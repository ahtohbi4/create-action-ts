{
  "name": "create-action-ts",
  "version": "0.6.1",
  "description": "Helper to create typed actions creators in React+Redux+TypeScript apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "tslint --config ./tslint.json --project ./tsconfig.json ./**/*.ts",
    "build": "rimraf dist && tsc",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahtohbi4/create-action-ts.git"
  },
  "author": {
    "name": "Alexandr Antonov",
    "email": "alexandr-post@yandex.ru"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "action",
    "action creator"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahtohbi4/create-action-ts/issues"
  },
  "homepage": "https://github.com/ahtohbi4/create-action-ts#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": ">=2.0"
  }
}
